#include <iostream>
#include <string>
using namespace std;
int main() { 

  string name;
    string email;

    cout << "Enter your full name";
    getline(cin, name);

    cout << "Enter your email address";
    getline(cin, email);

    cout << "Name" << name << endl;
    cout << "Email" << email << endl;

    return 0;
}
